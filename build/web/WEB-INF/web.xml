<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AccountSignIn</servlet-name>
        <servlet-class>controllers.AccountSignIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountSignUp</servlet-name>
        <servlet-class>controllers.AccountSignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListCatgories</servlet-name>
        <servlet-class>controllers.ListCatgories</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListProduct</servlet-name>
        <servlet-class>controllers.ListProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomerList</servlet-name>
        <servlet-class>controllers.CustomerList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailPro</servlet-name>
        <servlet-class>controllers.DetailPro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageProduct</servlet-name>
        <servlet-class>controllers.ManageProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditController</servlet-name>
        <servlet-class>controllers.EditController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteController</servlet-name>
        <servlet-class>controllers.DeleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateController</servlet-name>
        <servlet-class>controllers.CreateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartController</servlet-name>
        <servlet-class>controllers.CartController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartAction</servlet-name>
        <servlet-class>controllers.CartAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfile</servlet-name>
        <servlet-class>controllers.EditProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controllers.OrderManager</servlet-name>
        <servlet-class>controllers.OrderManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controllers.Cancel_order</servlet-name>
        <servlet-class>controllers.Cancel_order</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountSignIn</servlet-name>
        <url-pattern>/account/signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountSignUp</servlet-name>
        <url-pattern>/account/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListCatgories</servlet-name>
        <url-pattern>/categories</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListProduct</servlet-name>
        <url-pattern>/product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerList</servlet-name>
        <url-pattern>/customer-info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailPro</servlet-name>
        <url-pattern>/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageProduct</servlet-name>
        <url-pattern>/manage-pro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditController</servlet-name>
        <url-pattern>/product-edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteController</servlet-name>
        <url-pattern>/product-delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateController</servlet-name>
        <url-pattern>/product-create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartController</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartAction</servlet-name>
        <url-pattern>/cart-action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfile</servlet-name>
        <url-pattern>/editprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controllers.OrderManager</servlet-name>
        <url-pattern>/manage-order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controllers.Cancel_order</servlet-name>
        <url-pattern>/cancel-order</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
<!--    <error-page>
    <error-code>404</error-code>
        <location>/Error.jsp</location>
    </error-page>-->
</web-app>
